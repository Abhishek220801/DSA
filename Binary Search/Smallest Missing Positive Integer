// Linear Approach
for(int i=0; i<n; i++){
    if(i!=arr[i]){
        cout<<i;
        break;
    }
}

// Binary Search Approach
int lo = 0, hi = n-1, ans = -1;
  while(lo<=hi){
      int mid = lo + (hi-lo)/2;
      if(arr[mid]==mid) lo = mid+1;
      else{
          ans = mid;
          hi = mid-1;
      }
  }
  cout<<ans;
