https://leetcode.com/problems/sliding-window-maximum/

//Brute Force (gives TLE)
class Solution {
public:
    vector<int> maxSlidingWindow(vector<int>& nums, int k) {
        int n = nums.size();
        vector<int> nge;
        for(int i=0; i<n-k+1; i++){
            int mx = INT_MIN;
            for(int j=i; j<i+k; j++){
                mx = max(mx, nums[j]);
            }
            nge.push_back(mx);
        }
        return nge;
    }
};
