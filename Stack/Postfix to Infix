#include<bits/stdc++.h>
using namespace std;

string solve(string val1, string val2, char ch){
    string s= "";
    s+=val1;
    s.push_back(ch);
    s+=val2;
    return s;
}

int main(){

    string s = "79+4*8/3-";

    stack<string> val;

    for(int i=0; i<s.length(); i++){
        if(s[i]>='0' && s[i]<='9') val.push(to_string(s[i]-'0'));
        else{
            string v2 = val.top();
            val.pop();
            string v1 = val.top();
            val.pop();
            string ans = solve(v1, v2, s[i]);
            val.push(ans);
        }
    }

    cout<<"Answer: "<<val.top();

    return 0;
}
