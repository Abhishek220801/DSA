//O(n) space in Recursive print (whether print or reverse print)

void reversePrint(Node* head){
    Node* temp = head;
    if(!temp) return;
    reversePrint(temp->next);
    cout<<temp->data<<" ";
}

//For Doubly LL - using prev pointer
void printRev(Node* tail){
    if(!tail) return;
    cout<<tail->data<<" ";
    printRev(tail->prev);
}
