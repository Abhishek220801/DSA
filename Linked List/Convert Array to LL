Node* arrToLL(int arr[], int n){
    if(n==0) return NULL;
    Node* head = NULL, *tail = NULL;
    for(int i=0;i<n;i++){
        if(head==NULL){
            head = new Node(arr[i]);
            tail = head;
        }
        else{
            Node* temp = new Node(arr[i]);
            tail ->next = temp;
            temp->prev = tail;
            tail = temp;
        }
    }
    return head;
}
