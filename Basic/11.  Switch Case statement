double areaSwitchCase(int ch, vector<double> a) {
	// Write your code here
	switch(ch){
		case 1:
			return M_PI*a[0]*a[0];  //M_PI works when Math library is included (maths_PI denotes value of PI used in Maths)
		case 2:
			return a[0]*a[1]; 
	}
}
