https://leetcode.com/problems/defanging-an-ip-address/

class Solution {
public:
    string defangIPaddr(string address) {
        int idx = 0;
        string ans = "";
        while(idx < address.size()){
            if(address[idx]=='.') ans+="[.]";
            else ans+=address[idx];
            idx++;
        }
        return ans;
    }
};
