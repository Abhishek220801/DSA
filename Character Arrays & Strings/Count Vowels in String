int countVowel(string &greeting){
    int count = 0;
    for(int i=0; i<greeting[i]!='\0'; i++){
        if(greeting[i]=='a' || greeting[i]=='e' ||greeting[i]=='i' ||greeting[i]=='o' ||greeting[i]=='u' ||
        greeting[i]=='A' ||greeting[i]=='E' ||greeting[i]=='I' ||greeting[i]=='O' ||greeting[i]=='U'){
            count++;
        }
    }
    return count;
}
