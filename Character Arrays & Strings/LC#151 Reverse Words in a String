https://leetcode.com/problems/reverse-words-in-a-string

class Solution {
public:
    string reverseWords(string s) {
        stringstream ss(s);
        string word;
        vector<string> v;
        string ans = "";
        while(ss>>word){
            v.push_back(word);
        }
        for(int i=v.size()-1;i>=0;i--){
            ans+=v[i]+" ";
        }
        ans = ans.substr(0,ans.size()-1);
        return ans;
    }
};
