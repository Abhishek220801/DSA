int secondLargestElement(int arr[],int n){
    int max = INT_MIN;
    int second_max = INT_MIN;

    for(int i=0;i<n;i++){
        if(arr[i]>max){
            max=arr[i];
        }
    }

    for(int i=0;i<n;i++){
        if(arr[i]>second_max && arr[i]!=max){
            second_max=arr[i];
        }
    }

    return second_max;
}
