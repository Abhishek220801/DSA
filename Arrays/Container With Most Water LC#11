//brute force - O(N^2) - TLE

int maxArea(vector<int>& height) {
    int mostWater = 0;
    for(int i=0;i<height.size();i++){
        for(int j=i+1;j<height.size();j++){
            int w = j-i;
            int h = min(height[i],height[j]);
            int area = h*w;
            mostWater = max(area, mostWater);
        }
    }
    return mostWater;
}
