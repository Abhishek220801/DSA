//using sort STL method (not allowed in this problem)
//TC - O(N*log(N)) , SC - O(N)

void moveNegatives(vector<int>& nums){
    int l,h;
    l=0, h=nums.size()-1;
    while(l<h){
        if(nums[l]<0){
            l++;
        } else if(nums[h]>0){
            h--;
        }else{
            swap(nums[l],nums[h]);
        }
    }
}
