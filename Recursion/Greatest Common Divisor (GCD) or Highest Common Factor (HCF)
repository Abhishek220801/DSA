// HCF(a,b) <= min(a,b)
// LCM(a,b) >= max(a,b)

// TC -> O(min(a,b))
int hcf(int a, int b){
    for(int i=min(a,b); i>=2; i--){
        if(a%i==0 && b%i==0) return i;
    }
    return 1;
}

// or 

// Much better method -> Euclid's Division Algorithm (Long Division Method) -> O(log(a+b))
int hcf(int a, int b){
    if(a==0) return b;
    return hcf(b%a,a);
}
