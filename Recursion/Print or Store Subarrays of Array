// Subarray => continuous members and in order

void printSubarray(int arr[], int n, int idx, vector<int> curr){
    if(idx==n){
        for(int i=0; i<curr.size(); i++){
            cout<<curr[i]<<" ";
        }
        cout<<endl;
        return;
    }
    printSubarray(arr,n,idx+1,curr);
    if(curr.empty() || arr[idx-1]==curr[curr.size()-1]){
        curr.push_back(arr[idx]);
        printSubarray(arr,n,idx+1,curr);
    }
}
