#include<iostream>
#include<algorithm>
using namespace std;

int main(){
    
    string s = "AZYZXBDJKX", str="";
    
    for(char ch=0; ch<s.length(); ch++){
        if(s[ch]>='X') str+=s[ch];
    }

    // Now str == 'ZYZXX'
    
    int n = str.length();
    for(int i=0; i<n-1; i++){
        bool flag = true;
        for(int j=0;j<n-1-i; j++){
            if(str[j]>str[j+1]) swap(str[j],str[j+1]);
            flag = false;
        }
        if(flag) break;
    }
    reverse(str.begin(),str.end()); // Final str == 'ZZYXX'
    cout<<str;
    
    return 0;
}
