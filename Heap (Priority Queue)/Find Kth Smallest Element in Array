#include<iostream>
#include<queue>
using namespace std;

int main() {

    priority_queue<int> pq;
    int arr[] = {10,20,-4,5,18,24,1,-7,56}; // 5
    int n = sizeof(arr)/4;
    int k = 4;

    for(int i=0; i<n; i++){
        pq.push(arr[i]);
        if(pq.size()>k) pq.pop();
    }

    cout<<pq.top()<<endl;

    return 0;
}
