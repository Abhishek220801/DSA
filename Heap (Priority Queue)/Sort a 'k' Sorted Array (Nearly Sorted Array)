#include<iostream>
#include<queue>
using namespace std;

int main()
{

    int arr[] = {6,5,3,2,8,10,9};
    int k = 3;

    int n = sizeof(arr)/4;

    vector<int> ans;
    priority_queue<int, vector<int>, greater<int>> pq;
    for(auto x: arr){
        pq.push(x);
        if(pq.size()>k){
            ans.push_back(pq.top());
            pq.pop();
        }
    }
    while(!pq.empty()){
        ans.push_back(pq.top());
        pq.pop();
    }

    for(auto x: ans){
        cout<<x<<" ";
    }


    return 0;
}
